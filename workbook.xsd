    <xs:sequence>
      <xs:element type="xs:string" name="companyName"/>
      <xs:element type="xs:string" name="position"/>
      <xs:element type="xs:string" name="salary"/>
      <xs:element type="xs:string" name="workFrom"/>
      <xs:element type="xs:string" name="workUntil"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="com.fipsoftware.workbook.model.WorkBookType">
    <xs:sequence>
      <xs:element type="xs:string" name="SocialSecurity"/>
      <xs:element type="xs:string" name="firstName"/>
      <xs:element type="xs:string" name="lastName"/>
      <xs:element type="xs:string" name="dateOfBirth"/>
      <xs:element type="xs:string" name="photo"/>
      <xs:element type="workHistoriesType" name="workHistories"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="workHistoriesType">
    <xs:sequence>
      <xs:element type="com.fipsoftware.workbook.model.WorkHistoryType" name="com.fipsoftware.workbook.model.WorkHistory"/>
    </xs:sequence>
